<h1> Listing Publications </h1><table>  <tr>    <th>Title</th>    <th>Description</th>    <th>Date</th>    <th>Imagen</th>    <th></th>    <th></th>    <th></th>  </tr><% @publications.each do |publication| %>  <tr>    <td><%= publication.title %></td>    <td><%= publication.description %></td>    <td><%= publication.date %></td>    <td><%= publication.image_file_name %></td>    <td><%= link_to 'Show', publication %></td>	<td><%= link_to 'Edit', edit_publication_path(publication) %></td>	<td><%= link_to 'Destroy', publication, :confirm => 'Are you sure?', :method => :delete %></td>  </tr><% end %></table><br /><%= link_to 'New Publication', new_publication_path %>